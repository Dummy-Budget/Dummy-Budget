<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/frame_3"
        android:layout_width="554dp"
        android:layout_height="64dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#6A9C89"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/questionnair"
            android:layout_width="190dp"
            android:layout_height="52dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="182dp"
            android:layout_marginEnd="182dp"
            android:layout_marginBottom="6dp"
            android:gravity="center"
            android:text="@string/questionnair"
            android:textColor="#E1FFE1"
            android:textSize="28sp"
            android:textStyle="bold" />
    </RelativeLayout>

    <View
        android:id="@+id/rectangle_1"
        android:layout_width="526dp"
        android:layout_height="805dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="12dp"
        android:background="@drawable/rectangle_1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/current_loc"
        android:layout_width="213dp"
        android:layout_height="38dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="116dp"
        android:gravity="top"
        android:text="@string/current_loc"
        android:textAppearance="@style/yearly_salary"
        app:layout_constraintStart_toStartOf="parent"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/yearly_sala"
        android:layout_width="175dp"
        android:layout_height="36dp"
        android:layout_marginTop="116dp"
        android:layout_marginEnd="36dp"
        android:gravity="top"
        android:text="@string/yearly_salary"
        android:textAppearance="@style/yearly_salary"
        app:layout_constraintEnd_toEndOf="parent"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>